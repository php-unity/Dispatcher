{
    "name": "unity/dispatcher",
    "description": "Unity framework requests dispatcher",
    "type": "library",
    "keywords": [ "unity-dispatcher", "http-message", "psr-7", "php"],
    "homepage": "https://github.com/unity-framework/Dispatcher",
    "license": "MIT",
    "authors": [
      {
        "name": "Eleandro Duzentos",
        "email": "eleandro@inbox.ru",
        "homepage": "https://github.com/e200",
        "role": "developer"
      }
    ],
    "minimum-stability": "dev",
    "prefer-stable" : true,
    "require": {
      "unity/contracts": "dev-master",
      "make/accessible": "dev-master"
    },
    "repositories": [
      {
        "type": "vcs",
        "url": "../contracts"
      },
      {
        "type": "vcs",
        "url": "../make_accessible"
      }
    ],
    "autoload": {
      "psr-4": {
        "Unity\\Component\\Dispatcher\\": "src/"
      }
    },
    "require-dev": {
      "phpunit/phpunit": "^6.4"
    }
  }
  